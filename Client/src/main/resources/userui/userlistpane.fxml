<fx:root type="AnchorPane" fx:id="userlist" prefHeight="560.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
<children>

            <JFXTextField focusColor="#8CA2F6" unFocusColor="#659EFB" layoutX="27.0" layoutY="28.0" prefHeight="30.0" prefWidth="150.0" />

    <JFXButton fx:id="filter" layoutX="420.0" layoutY="35.0" mnemonicParsing="false" text="Filter">
        <graphic>
            <HBox>
                <children>
                    <JFXRippler position="BACK" styleClass="icons-rippler">
                        <StackPane>
                            <FontAwesomeIconView fill="white" glyphName="NAVICON" size="1em" styleClass="icon" />
                        </StackPane>
                    </JFXRippler>
                </children>
            </HBox>
        </graphic>
    </JFXButton>
            <JFXRippler fx:id="search" ripplerFill="#DA4CEE" layoutX="177" layoutY="22" maskType="CIRCLE" position="BACK" styleClass="icons-rippler">
                <StackPane style="-fx-padding: 8;" translateY="5">
                    <OctIconView style="-fx-fill: linear-gradient(to top right , rgba(170,17,242,0.7),rgba(218,76,238,0.7));" glyphName="SEARCH" size="1.5em" styleClass="icon" />
                </StackPane>
            </JFXRippler>


            <JFXButton fx:id="delete" layoutX="565.0" layoutY="35.0" onMouseClicked="#deleteList" mnemonicParsing="false"  text="Delete" />



            <JFXButton fx:id="add" onMouseClicked="#add" layoutX="494.0" layoutY="35.0" mnemonicParsing="false" text="Add">
                <graphic>
                    <HBox>
                        <children>
                            <JFXRippler position="BACK" styleClass="icons-rippler">
                                <StackPane>
                                    <FontAwesomeIconView fill="white" glyphName="PLUS" size="1em" styleClass="icon" />
                                </StackPane>
                            </JFXRippler>
                        </children>
                    </HBox>
                </graphic>
            </JFXButton>


                    <BorderPane fx:id="borderpane" layoutX="20.0" layoutY="80.0" prefHeight="480.0" prefWidth="600.0"/>





