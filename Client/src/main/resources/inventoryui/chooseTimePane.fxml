<fx:root prefHeight="295.0" prefWidth="329.0" stylesheets="@../css/add.css" type="AnchorPane" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
<children>
    <Label layoutX="0" layoutY="0" text="选择时间区间">
        <font>
            <Font name="Microsoft YaHei Bold" size="24.0" />
        </font></Label>

    <VBox alignment="CENTER" layoutX="65.0" layoutY="48.0" prefHeight="247.0" prefWidth="200.0">
        <children>


        <Label prefHeight="49.0" prefWidth="200.0" text="开始时间" VBox.vgrow="SOMETIMES">
            <font>
               <Font name="Microsoft YaHei Bold" size="20.0" />
        </font>
            <VBox.margin>
               <Insets />
            </VBox.margin></Label>

        <JFXDatePicker fx:id="startTimeField" prefHeight="35.0" prefWidth="200.0">
            <VBox.margin>
               <Insets />
            </VBox.margin></JFXDatePicker>

        <Label prefHeight="40.0" prefWidth="212.0" text="结束时间">
            <font>
                <Font name="Microsoft YaHei Bold" size="20.0" />
            </font></Label>

        <JFXDatePicker fx:id="endTimeField" prefHeight="41.0" prefWidth="200.0" />

        <JFXButton fx:id="sure" alignment="BOTTOM_CENTER" layoutX="44.0" layoutY="47.0" onMouseClicked="#sureTime" prefHeight="36.0" prefWidth="88.0" ripplerFill="#252424" style="-fx-background-color: slateblue;" text="确认" textAlignment="CENTER" textFill="WHITE">
