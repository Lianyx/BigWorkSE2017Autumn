<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXRippler?>
<?import com.jfoenix.controls.JFXTextField?>
<?import de.jensd.fx.glyphs.materialdesignicons.MaterialDesignIconView?>
<?import de.jensd.fx.glyphs.octicons.OctIconView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<fx:root fx:id="inventorycheckpane" prefHeight="560.0" prefWidth="640.0" type="AnchorPane" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <children>

        <JFXTextField fx:id="searchField" focusColor="#8CA2F6" layoutX="27.0" layoutY="28.0" prefHeight="30.0" prefWidth="150.0" />
        <JFXRippler fx:id="search" layoutX="177" layoutY="22" maskType="CIRCLE" onMouseClicked="#search" position="BACK" ripplerFill="#03A9F4" styleClass="icons-rippler">
            <StackPane style="-fx-padding: 8;" translateY="5">
                <OctIconView glyphName="SEARCH" size="1.5em" styleClass="icon" />
            </StackPane>
        </JFXRippler>


        <JFXButton fx:id="export" layoutX="518.0" layoutY="28.0" mnemonicParsing="false" onMouseClicked="#exportExcel" text="导出EXCEL">
            <graphic>
                <HBox>
                    <children>
                        <JFXRippler position="BACK" styleClass="icons-rippler">
                            <StackPane>
                                <MaterialDesignIconView fill="white" glyphName="DELETE" size="1em" styleClass="icon" />
                            </StackPane>
                        </JFXRippler>
                    </children>
                </HBox>
            </graphic>
        </JFXButton>


        <BorderPane fx:id="borderpane" layoutX="20.0" layoutY="80.0" prefHeight="480.0" prefWidth="600.0" />


    </children>
</fx:root>
